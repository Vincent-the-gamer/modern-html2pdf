{
  "name": "modern-html2pdf",
  "version": "0.1.1",
  "type": "module",
  "description": "Converts any HTML element into a PDF entirely using modern-screenshot and jsPDF",
  "author": "Vincent-the-gamer <erxianqiaottjt@163.com>",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "license": "MIT",
  "homepage": "https://github.com/Vincent-the-gamer/modern-html2pdf#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vincent-the-gamer/modern-html2pdf.git"
  },
  "bugs": {
    "url": "https://github.com/Vincent-the-gamer/modern-html2pdf/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "sideEffects": false,
  "keywords": [
    "html2pdf"
  ],
  "dependencies": {
    "jspdf": "^2.5.1",
    "modern-screenshot": "^4.4.39"
  },
  "devDependencies": {
    "tsup": "^8.2.3",
    "typescript": "^5.5.4"
  }
}
